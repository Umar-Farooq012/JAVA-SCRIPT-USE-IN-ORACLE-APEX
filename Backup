SELECT
VOUCHER_DET_ID APPROVAL_ID, DFD.BATCH_NO, DFD.BATCH_MARKETING_NAME, DFD.FILE_NO,
DIWR.BOOKING_ID,
DTP.STATUS,
INITCAP(DIWR.CLIENT) CLIENT,
DPD.PLOT_NO,
DIWR.DR_AMOUNT AMOUNT,
INITCAP(DIWR.APPROVAL_STATUS)  APPROVAL_STATUS,
--'VIEW DETAILS' DETAILS,
CASE  WHEN RECIEPT_MODE = 'CHEQUE' THEN 'Instrument' ELSE INITCAP(RECIEPT_MODE) END "RECEIPT MODE",
INITCAP(TRANSACTION_TYPE) TRANSACTION_TYPE,
CHEQUE_DATE "INSTRUMENT DATE",
CHEQUE_NO "INSTRUMENT NO",
INITCAP(NVL(BANK_ACCOUNT,CASH_ACCOUNT)) ACCOUNT_NAME,
--'<b>'||TAX_STAT||'</b>' "RECEIPT WITH TAX?",
--BEFORE_TAX_AMT "WITHOUT TAX",
--TAX_AMT "TAX AMOUNT",
RECEIPT_ID,
--CASE WHEN DIWR.APPROVAL_STATUS = 'ACCEPTED' OR DIWR.APPROVAL_STATUS = 'REJECTED' THEN NULL ELSE  'PRINT' END PRINT,
DIWR.PAID_DATE,
--CASE WHEN DIWR.APPROVAL_STATUS = 'ACCEPTED' OR DIWR.APPROVAL_STATUS = 'REJECTED' THEN NULL ELSE DIWR.PAID_DATE END CHANGE_DATE,
 DFD.BATCH_MARKETING_NAME||' - '||DFD.BATCH_NO BATCH,
 'Refund' Refund
FROM DGMS_VOUCHERS_APPROVAL DIWR 
LEFT JOIN DGMS_FILES_DETAILS DFD ON DFD.ID=DIWR.PLOT_ID	 AND DFD.ORG_ID = :P_ORG_ID
LEFT JOIN DGMS_PLOTS_BOOKING DPB ON  DPB.PLOT_ID=DIWR.PLOT_ID AND BOOKING_STATUS = 'BOOKED' AND DPB.ORG_ID = :P_ORG_ID
LEFT JOIN DGMS_PLOT_DETAILS DPD ON  DPB.PLOT_ID=DPD.ID AND DPD.ORG_ID = :P_ORG_ID
LEFT JOIN DGMS_TRANSFERRED_PLOT DTP ON DPD.ID = DTP.PLOT_ID 

--LEFT JOIN DGMS_FILES_DETAILS DFD ON DFD.ID =DPD.FILE_ID AND DFD.ORG_ID=:P_ORG_ID
WHERE  DIWR.ORG_ID = :P_ORG_ID
AND  TRANSACTION_TYPE  = 'TRANSFER FEES' 
AND DIWR.APPROVAL_STATUS = 'ACCEPTED'
AND DTP.STATUS != 'TRANSFERRED'













